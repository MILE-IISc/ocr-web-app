    <block type="Text" BlockNumber="1" SubType="pageFooter" rowStart="1967" rowEnd="2019" colStart="610" colEnd="1279">
        <line LineNumber="1" rowStart="1967" rowEnd="2019" colStart="610" colEnd="1279">
            <word unicode="ಬಾಹ್ಯಾಕಾಶಯಾನಿಗಳು ತೇಲಾಡುವುದೇಕೆ?/" WordNumber="1" rowStart="1967" rowEnd="2019" colStart="610" colEnd="1253"/>
            <word unicode="7" WordNumber="2" rowStart="1971" rowEnd="2000" colStart="1255" colEnd="1279"/>
        </line>
    </block>
    <block type="Text" BlockNumber="2" SubType="paragraphProse" rowStart="190" rowEnd="381" colStart="212" colEnd="1273">
        <line LineNumber="2" rowStart="190" rowEnd="228" colStart="271" colEnd="1270">
            <word unicode="ನರಸಿಂಹನ ದರ್ಶನ ಮುಗಿಸಿಕೊಂಡು ಗುಹೆಯಿಂದ ಹೊರಗೆ" WordNumber="1" rowStart="190" rowEnd="228" colStart="271" colEnd="1270"/>
        </line>
        <line LineNumber="3" rowStart="263" rowEnd="318" colStart="212" colEnd="1273">
            <word unicode="ಬಂದ ಮೇಲೆ ಮತ್ತೆ ಪುಟ್ಟನಲ್ಲಿ ಚಿಂತನೆ ಶುರುವಾಯಿತು. ಕತ್ತಲಲ್ಲೂ" WordNumber="1" rowStart="263" rowEnd="318" colStart="212" colEnd="1273"/>
        </line>
        <line LineNumber="4" rowStart="336" rowEnd="381" colStart="212" colEnd="989">
            <word unicode="ಬಾವಲಿಗಳು ಸರಾಗವಾಗಿ ಹಾರಾಡುವುದು ಹೇಗೆ?" WordNumber="1" rowStart="336" rowEnd="381" colStart="212" colEnd="989"/>
        </line>
    </block>
    <block type="Text" BlockNumber="3" SubType="paragraphProse" rowStart="409" rowEnd="1125" colStart="212" colEnd="1277">
        <line LineNumber="5" rowStart="409" rowEnd="463" colStart="271" colEnd="1270">
            <word unicode="ನಮಗೆ ಯಾವುದೇ ವಸ್ತು ಕಾಣಿಸಬೇಕಾದರೆ ಮೊದಲು ಬೆಳಕು" WordNumber="1" rowStart="409" rowEnd="463" colStart="271" colEnd="1270"/>
        </line>
        <line LineNumber="6" rowStart="482" rowEnd="538" colStart="212" colEnd="1275">
            <word unicode="ಆ ವಸ್ತುವಿನ ಮೇಲೆ ಬೀಳಬೇಕು. ಆ ವಸ್ತು ತನ್ನ ಮೇಲೆ ಬಿದ್ದ" WordNumber="1" rowStart="482" rowEnd="538" colStart="212" colEnd="1275"/>
        </line>
        <line LineNumber="7" rowStart="555" rowEnd="611" colStart="212" colEnd="1277">
            <word unicode="ಬೆಳಕನ್ನು ಪ್ರತಿಫಲಿಸಬೇಕು. ಪ್ರತಿಫಲಿತ ಬೆಳಕು ನಮ್ಮ ಕಣ್ಣನ್ನು" WordNumber="1" rowStart="555" rowEnd="611" colStart="212" colEnd="1277"/>
        </line>
        <line LineNumber="8" rowStart="627" rowEnd="681" colStart="212" colEnd="1270">
            <word unicode="ಪ್ರವೇಶಿಸಬೇಕು. ಆಗ ಆ ವಸ್ತು ನಮಗೆ ಕಾಣಿಸುತ್ತದೆ. ಇಲ್ಲವಾದರೆ" WordNumber="1" rowStart="627" rowEnd="681" colStart="212" colEnd="1270"/>
        </line>
        <line LineNumber="9" rowStart="701" rowEnd="755" colStart="212" colEnd="1271">
            <word unicode="ಇಲ್ಲ. ಗುಹೆಯಲ್ಲಿ ಬೆಳಕಿಲ್ಲ. ಅಲ್ಲಿನ ಯಾವ ವಸ್ತುಗಳು ಸ್ಪಷ್ಟವಾಗಿ" WordNumber="1" rowStart="701" rowEnd="755" colStart="212" colEnd="1271"/>
        </line>
        <line LineNumber="10" rowStart="773" rowEnd="830" colStart="212" colEnd="1271">
            <word unicode="ಕಾಣಿಸುತ್ತಿರಲಿಲ್ಲ. ಭಕ್ತರೆಲ್ಲ ಕುರುಡರಂತೆ ಒಬ್ಬರ ಹಿಂದೆ ಒಬ್ಬರು" WordNumber="1" rowStart="773" rowEnd="830" colStart="212" colEnd="1271"/>
        </line>
        <line LineNumber="11" rowStart="847" rowEnd="901" colStart="212" colEnd="1275">
            <word unicode="ತಡಕಾಡುತ್ತಾ ನಡೆಯುತ್ತಿದ್ದರು. ಅಂಥದ್ದರಲ್ಲಿ ಬಾವಲಿಗಳು ಅಷ್ಟು" WordNumber="1" rowStart="847" rowEnd="901" colStart="212" colEnd="1275"/>
        </line>
        <line LineNumber="12" rowStart="920" rowEnd="976" colStart="212" colEnd="1273">
            <word unicode="ಸರಾಗವಾಗಿ ಹಾರಾಡಲು ಕಾರಣವೇನು? ಅವುಗಳ ಕಣ್ಣುಗಳಲ್ಲಿ" WordNumber="1" rowStart="920" rowEnd="976" colStart="212" colEnd="1273"/>
        </line>
        <line LineNumber="13" rowStart="992" rowEnd="1048" colStart="212" colEnd="1270">
            <word unicode="ಏನಾದರೂ ವಿಶೇಷವಿದೆಯೇ? ಅಥವಾ, ವಸ್ತುಗಳನ್ನು ಗ್ರಹಿಸಲು" WordNumber="1" rowStart="992" rowEnd="1048" colStart="212" colEnd="1270"/>
        </line>
        <line LineNumber="14" rowStart="1065" rowEnd="1125" colStart="212" colEnd="1200">
            <word unicode="ಏನಾದರೂ ವಿಶೇಷ ವ್ಯವಸ್ಥೆ ಇದೆಯೇ? ಉತ್ತರ ಹೊಳೆಯಲಿಲ್ಲ." WordNumber="1" rowStart="1065" rowEnd="1125" colStart="212" colEnd="1200"/>
        </line>
    </block>
    <block type="Text" BlockNumber="4" SubType="paragraphProse" rowStart="1138" rowEnd="1260" colStart="212" colEnd="1275">
        <line LineNumber="15" rowStart="1138" rowEnd="1194" colStart="271" colEnd="1275">
            <word unicode="ಮನೆಗೆ ಹಿಂದಿರುಗಿದ ಮೇಲೆ ಕಿಟ್ಟಣ್ಣನನ್ನು ಕೇಳಿದ. ಕಿಟ್ಟಣ್ಣ" WordNumber="1" rowStart="1138" rowEnd="1194" colStart="271" colEnd="1275"/>
        </line>
        <line LineNumber="16" rowStart="1213" rowEnd="1260" colStart="212" colEnd="614">
            <word unicode="ವಿವರಿಸಲು ಪ್ರಾರಂಭಿಸಿದ." WordNumber="1" rowStart="1213" rowEnd="1260" colStart="212" colEnd="614"/>
        </line>
    </block>
    <block type="Text" BlockNumber="5" SubType="paragraphProse" rowStart="1284" rowEnd="1556" colStart="212" colEnd="1275">
        <line LineNumber="17" rowStart="1284" rowEnd="1340" colStart="271" colEnd="1270">
            <word unicode="“ಪುಟ್ಟ, ಬಾವಲಿಗಳಿಗೆ ವಿಶೇಷವಾದ ಕಣ್ಣುಗಳೇನೂ ಇಲ್ಲ. ಆದರೆ" WordNumber="1" rowStart="1284" rowEnd="1340" colStart="271" colEnd="1270"/>
        </line>
        <line LineNumber="18" rowStart="1357" rowEnd="1417" colStart="212" colEnd="1270">
            <word unicode="ವಸ್ತುಗಳ ಇರುವಿಕೆಯನ್ನು ಗ್ರಹಿಸಲು ವಿಶೇಷವಾದ ವ್ಯವಸ್ಥೆಯಂತೂ" WordNumber="1" rowStart="1357" rowEnd="1417" colStart="212" colEnd="1270"/>
        </line>
        <line LineNumber="19" rowStart="1430" rowEnd="1486" colStart="212" colEnd="1275">
            <word unicode="ಇದೆ. ಅದರಿಂದಲೇ ಅವು ಯಾವುದೇ ಅಡೆತಡೆಗೂ ಡಿಕ್ಕಿ" WordNumber="1" rowStart="1430" rowEnd="1486" colStart="212" colEnd="1275"/>
        </line>
        <line LineNumber="20" rowStart="1503" rowEnd="1556" colStart="212" colEnd="872">
            <word unicode="ಹೊಡೆಯದೆ ಸರಾಗವಾಗಿ ಹಾರಾಡುತ್ತವೆ.”" WordNumber="1" rowStart="1503" rowEnd="1556" colStart="212" colEnd="872"/>
        </line>
    </block>
    <block type="Text" BlockNumber="6" SubType="paragraphProse" rowStart="1576" rowEnd="1636" colStart="271" colEnd="923">
        <line LineNumber="21" rowStart="1576" rowEnd="1636" colStart="271" colEnd="923">
            <word unicode="“ಏನದು ವಿಶೇಷ ವ್ಯವಸ್ಥೆ?” ಪುಟ್ಟ ಕೇಳಿದ." WordNumber="1" rowStart="1576" rowEnd="1636" colStart="271" colEnd="923"/>
        </line>
    </block>
    <block type="Text" BlockNumber="7" SubType="paragraphProse" rowStart="1649" rowEnd="1850" colStart="212" colEnd="1277">
        <line LineNumber="22" rowStart="1649" rowEnd="1704" colStart="271" colEnd="1277">
            <word unicode="“ಯಾವುದೇ ವಸ್ತುವಿನ ಇರುವಿಕೆ ಅಥವಾ ಇಲ್ಲದಿರುವಿಕೆಯನ್ನು" WordNumber="1" rowStart="1649" rowEnd="1704" colStart="271" colEnd="1277"/>
        </line>
        <line LineNumber="23" rowStart="1722" rowEnd="1778" colStart="212" colEnd="1270">
            <word unicode="ಕಂಡುಕೊಳ್ಳಲು ನಮ್ಮ ಪಂಚೇಂದ್ರಿಯಗಳು ನಮಗೆ ಸಹಾಯ" WordNumber="1" rowStart="1722" rowEnd="1778" colStart="212" colEnd="1270"/>
        </line>
        <line LineNumber="24" rowStart="1794" rowEnd="1850" colStart="212" colEnd="1163">
            <word unicode="ಮಾಡುತ್ತವೆ. ಹೌದಲ್ಲವೆ?” ಕಿಟ್ಟಣ್ಣ ಪ್ರಶ್ನೆಗೊಂದು ಪ್ರಶ್ನೆ ಎಸೆದ." WordNumber="1" rowStart="1794" rowEnd="1850" colStart="212" colEnd="1163"/>
        </line>
    </block>
    <block type="Text" BlockNumber="8" SubType="paragraphProse" rowStart="1867" rowEnd="1922" colStart="271" colEnd="631">
        <line LineNumber="25" rowStart="1867" rowEnd="1922" colStart="271" colEnd="631">
            <word unicode="“ಹೌದು” ಪುಟ್ಟ ಒಪ್ಪಿದ." WordNumber="1" rowStart="1867" rowEnd="1922" colStart="271" colEnd="631"/>
        </line>
    </block>
</page>
